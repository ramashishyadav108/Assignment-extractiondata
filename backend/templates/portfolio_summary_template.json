{
  "template_id": "portfolio_summary_template",
  "name": "Portfolio Summary Extraction Template",
  "description": "Comprehensive fund data extraction with page references",
  "output_format": "vertical",
  "sheets": [
    {
      "name": "Portfolio Summary",
      "fields": [
        {
          "field": "General Partner",
          "description": "Name of the General Partner managing the fund",
          "required": false
        },
        {
          "field": "ILPA GP",
          "description": "ILPA format General Partner name",
          "required": false
        },
        {
          "field": "Assets Under Management",
          "description": "Total assets under management (numeric value)",
          "type": "currency",
          "required": false
        },
        {
          "field": "Active Funds",
          "description": "Number of active funds",
          "type": "number",
          "required": false
        },
        {
          "field": "Active Portfolio Companies",
          "description": "Number of active portfolio companies",
          "type": "number",
          "required": false
        },
        {
          "field": "Fund Name",
          "description": "Official name of the fund",
          "required": true
        },
        {
          "field": "Fund Currency",
          "description": "Currency used for fund reporting (e.g., USD, EUR)",
          "required": false
        },
        {
          "field": "Total Commitments",
          "description": "Total committed capital",
          "type": "currency",
          "required": false
        },
        {
          "field": "Total Drawdowns",
          "description": "Total capital drawn/paid-in",
          "type": "currency",
          "required": false
        },
        {
          "field": "Remaining Commitments",
          "description": "Uncalled capital remaining",
          "type": "currency",
          "required": false
        },
        {
          "field": "Total Number of Investments",
          "description": "Total number of portfolio investments",
          "type": "number",
          "required": false
        },
        {
          "field": "Total Distributions",
          "description": "Total distributions to investors",
          "type": "currency",
          "required": false
        },
        {
          "field": "- as % of Drawdowns",
          "description": "Distributions as percentage of drawdowns",
          "type": "percentage",
          "required": false
        },
        {
          "field": "- as % of Commitments",
          "description": "Distributions as percentage of commitments",
          "type": "percentage",
          "required": false
        },
        {
          "field": "DPI",
          "description": "Distributions to Paid-In Capital multiple",
          "type": "number",
          "required": false
        },
        {
          "field": "RVPI",
          "description": "Residual Value to Paid-In Capital multiple",
          "type": "number",
          "required": false
        },
        {
          "field": "TVPI",
          "description": "Total Value to Paid-In Capital multiple",
          "type": "number",
          "required": false
        }
      ]
    }
  ]
}
