# Application Settings
APP_NAME=PDF Extraction System
APP_VERSION=1.0.0
DEBUG=False

# API Keys (REQUIRED - Add your keys here)
GEMINI_API_KEY=your_gemini_api_key_here

# LLM Configuration
LLM_MODEL=gemini-2.5-flash
LLM_TEMPERATURE=0.1
LLM_MAX_RETRIES=3

# Database (Optional - for Neon PostgreSQL)
DATABASE_URL=

# File Upload Configuration
MAX_UPLOAD_SIZE=52428800
UPLOAD_DIR=examples/sample_pdfs
OUTPUT_DIR=examples/output

# CORS Settings (Update with your Vercel frontend URL after deployment)
CORS_ORIGINS=["http://localhost:3000", "http://localhost:5173", "https://your-app.vercel.app"]

# Template Directory
TEMPLATE_DIR=templates

